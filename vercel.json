{
    "version": 2,
    "builds": [
        {
            "src": "public/index.php",
            "use": "@vercel/php"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "CACHE_DRIVER": "file",
        "SESSION_DRIVER": "file",
        "QUEUE_DRIVER": "sync",
        "LOG_CHANNEL": "stack"
    },
    "functions": {
        "public/index.php": {
            "runtime": "vercel-php@0.6.0"
        }
    }
} 